{
  "name": "bk-electron",
  "version": "1.0.0",
  "description": "betterkonachan electron version",
  "main": "dist/main.js",
  "scripts": {
    "dist": "npx rimraf dist/* && npm run main:build && npm run render:build && npm run copy && electron-builder --win --x64",
    "convert": "convert -density 512x512 -background transparent ./assets/image/icon.svg -define icon:auto-resize -colors 512 ./assets/image/icon.ico",
    "copy": "copyfiles -f ./assets/image/splash.svg ./assets/image/icon.ico dist",
    "render:dev": "webpack-dev-server --inline --config ./webpack/render.dev.ts",
    "render:build": "webpack --config ./webpack/render.prod.ts",
    "lint": "eslint ./**/*.{ts,tsx,js}",
    "prettier": "prettier ./**/*.{ts,tsx,js,css} --write",
    "test": "jest",
    "codecov": "codecov",
    "main:start": "electron .",
    "main:watch": "webpack --mode development --watch --config ./webpack/main.ts",
    "main:build": "webpack --mode production --config ./webpack/main.ts"
  },
  "keywords": [
    "betterkonachan",
    "electron"
  ],
  "author": "franciscowxp@github.com",
  "build": {
    "appId": "BetterKonacha",
    "files": [
      "./dist/**/*"
    ],
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "icon": "dist/icon.ico"
    }
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.3",
    "electron-download-manager": "^2.1.2",
    "@trodi/electron-splashscreen": "^1.0.0",
    "electron-store": "^5.1.1",
    "electron-unhandled": "^3.0.2",
    "electron-window-state": "^5.0.3",
    "is-image": "^3.0.0",
    "mousetrap": "^1.6.5",
    "node-vibrant": "^3.1.5",
    "ramda": "^0.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-perfect-scrollbar": "^1.5.8",
    "react-spinkit": "^3.0.0",
    "react-transition-group": "^4.4.1",
    "react-use": "^15.1.1",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "^6.5.5",
    "rxjs-hooks": "^0.6.2",
    "rxjs-retry-delay": "^0.3.1",
    "typescript": "^3.9.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.0.5",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/cheerio": "^0.22.18",
    "@types/electron-window-state": "^2.0.34",
    "@types/html-webpack-plugin": "^3.2.3",
    "@types/jest": "^25.2.3",
    "@types/mousetrap": "^1.6.3",
    "@types/node": "^14.0.9",
    "@types/ramda": "^0.27.6",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-spinkit": "^3.0.6",
    "@types/react-transition-group": "^4.4.0",
    "@types/webpack": "^4.41.17",
    "@types/webpack-dev-server": "^3.11.0",
    "@types/webpack-env": "^1.15.2",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "awesome-typescript-loader": "^5.2.1",
    "canvas": "^2.6.1",
    "concurrently": "^5.2.0",
    "copyfiles": "^2.3.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "electron": "^9.0.1",
    "electron-builder": "^22.7.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "jsdom-global": "^3.0.2",
    "node-pre-gyp": "^0.15.0",
    "postcss-assets": "^5.0.0",
    "postcss-color-function": "^4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^9.0.0",
    "postcss-preset-env": "^6.7.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "source-map-loader": "^1.0.0",
    "style-loader": "^1.1.3",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "tslint-microsoft-contrib": "^6.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  }
}
